<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.air.monitor">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.FindPwdActivity"/>
        <activity android:name=".ui.RegisterActivity"/>
        <activity android:name=".ui.ContactActivity"/>
        <activity android:name=".ui.CreateContactActivity"/>
        <activity android:name=".ui.RecordListActivity"/>
        <activity android:name=".ui.ChartActivity"/>
        <activity android:name=".ui.DataActivity"/>
    </application>

</manifest>